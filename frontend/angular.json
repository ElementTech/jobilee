{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "jobilee": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true

        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "lodash",
              "rxjs-compat",
              "@ks89/angular-modal-gallery",
              "@ckeditor/ckeditor5-build-classic",
              "clone-deep",
              "dropzone",
              "leaflet",
              "feather-icons",
              "mousetrap",
              "hammerjs",
              "firebase/app",
              "dragula",
              "knob",
              "chartist",
              "chartjs-plugin-streaming",
              "@firebase/app",
              "chart.js",
              "dom-set",
              "crossvent",
              "contra/emitter",
              "ev-emitter/ev-emitter",
              "get-size/get-size",
              "outlayer/outlayer",
              "rxjs",
              "ngx-masonry",
              "@firebase/component",
              "@firebase/firestore",
              "@firebase/auth",
              "@firebase/util",
              "rxjs/operators",
              "rxjs/Subject",
              "rxjs/internal/operators",
              "rxjs/internal/observable/of",
              "rxjs/add/operator/map",
              "rxjs/Subscription",
              "masonry-layout",
              "imagesloaded",
              "rxjs-compat/add/operator/map",
              "@mattlewis92/dom-autoscroller",
              "dom-plane",
              "@mattlewis92/dom-autoscroller",
              "@tweenjs/tween.js",
              "photoswipe",
              "photoswipe/dist/photoswipe-ui-default",
              "angular-calendar/date-adapters/date-fns",
              "apexcharts",
              "apexcharts/dist/apexcharts.min.js",
              "calendar-utils/date-adapters/date-fns"
            ],
            "outputPath": "dist/jobilee",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets/images/favicon.png",
              {
                "glob": "**/*",
                "input": "src/assets/",
                "ignore": ["**/*.scss"],
                "output": "/assets/"
              },
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/font-awesome/scss/font-awesome.scss",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css",
              "src/styles.scss",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/nova-alt/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeflex/primeflex.css"
            ],
            "scripts": [
              "node_modules/chart.js/dist/chart.js",
              "node_modules/marked/marked.min.js",
              "node_modules/quill/dist/quill.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "9mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "9mb",
                  "maximumError": "9mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "jobilee:build",
            "proxyConfig": "src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "jobilee:build:production",
              "proxyConfig": "src/proxy.conf.json"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "jobilee:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "jobilee:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "jobilee:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "jobilee",
  "cli": {
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  }
}
